{
    "version": 1,
    "formatters": {
        "default": {"format": "%(asctime)s %(name)s %(levelname)s: %(message)s", "datefmt": "%Y-%m-%d %H:%M:%S"}
    },
    "handlers": {
        "file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "formatter": "default",
            "filename": "test.log",
            "maxBytes": 10485760,
            "mode": "a",
            "backupCount": 5
        }
    },
    "loggers": {
        "": {
            "level": "DEBUG",
            "handlers": ["file"]
        }
    },
    "disable_existing_loggers": true
}